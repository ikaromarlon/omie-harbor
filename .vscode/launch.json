  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start HTTP server",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "offline",
        "--noTimeout",
        "--noAuth",
        "cloudside"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke ingestionDispatcher",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "ingestionDispatcher"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke ingestionPerformer",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "ingestionPerformer",
        "-p", "${workspaceFolder}/.vscode/events/ingestionPerformer.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke webhook",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "webhook",
        "-p", "${workspaceFolder}/.vscode/events/webhook.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke deleteDataByCompany",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "deleteDataByCompany",
        "-p", "${workspaceFolder}/.vscode/events/deleteDataByCompany.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke dataExport - EventBridge",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "dataExport",
        "-p", "${workspaceFolder}/.vscode/events/dataExport-eventBridge.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke dataExport - SQS",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "dataExport",
        "-p", "${workspaceFolder}/.vscode/events/dataExport-sqs.json.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run bin [dev]",
      "program": "${workspaceFolder}/bin/index.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [],
      "env": {
        "STAGE": "dev"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run bin [prd]",
      "program": "${workspaceFolder}/bin/index.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [],
      "env": {
        "STAGE": "prd"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke dataProcessing function",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "invoke",
        "local",
        "-f", "dataProcessing",
        "-p", "${workspaceFolder}/.vscode/events/dataProcessing.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "SLS_DEBUG": "*"
      },
    },
  ]
}