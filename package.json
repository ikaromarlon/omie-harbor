{
  "name": "omie-harbor",
  "version": "1.8.1",
  "description": "A serveless application for ingestion and parsing data from Omie ERP and deliver it through a REST API",
  "main": "src/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "sls offline",
    "dev": "nodemon -e js --exec \"npm start\"",
    "debug": "SLS_DEBUG=* node --inspect node_modules/serverless/bin/serverless offline --noTimeout",
    "test": "jest --runInBand --silent --passWithNoTests",
    "deploy": "sls deploy --verbose",
    "lint": "standard --fix",
    "lint:staged": "lint-staged",
    "test:unit": "npm test -- --config jest.config.unit.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm run test:unit -- --collectCoverage",
    "prepare": "husky install"
  },
  "author": {
    "name": "Icaro Silva",
    "email": "ikaromarlon@gmail.com",
    "url": "https://www.linkedin.com/in/ikaromarlon/"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.3",
    "joi": "^17.11.0",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.484.0",
    "@aws-sdk/client-ses": "^3.484.0",
    "@aws-sdk/client-sqs": "^3.484.0",
    "@shelf/jest-mongodb": "^4.2.0",
    "@types/jest": "^29.5.11",
    "dotenv": "^16.3.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "serverless": "^3.38.0",
    "serverless-apigateway-service-proxy": "^2.2.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^13.3.2",
    "standard": "^17.1.0"
  },
  "standard": {
    "env": [
      "node",
      "jest"
    ]
  }
}
