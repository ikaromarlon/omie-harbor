{
  "name": "omie-harbor",
  "version": "1.8.1",
  "description": "A serveless application for ingestion and parsing data from Omie ERP and deliver it through a REST API",
  "main": "src/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "sls offline cloudside",
    "lint": "standard",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "test": "jest --no-cache --silent --passWithNoTests",
    "test:unit": "npm test -- --config jest.config.unit.js",
    "test:integration": "npm test -- --config jest.config.integration.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm run test:unit -- --collectCoverage && npm run test:integration -- --collectCoverage",
    "bin:dev": "STAGE=dev node ./bin/index.js",
    "bin:prd": "STAGE=prd node ./bin/index.js",
    "ci": "npm ci && npm run test:ci",
    "ci:dev": "npm run ci && npm run bin:dev -- --createIndexes",
    "ci:prd": "npm run ci && npm run bin:prd -- --createIndexes",
    "predeploy:dev": "npm run ci:dev",
    "predeploy:prd": "npm run ci:prd",
    "deploy": "sls deploy --verbose",
    "deploy:dev": "npm run deploy -- --stage dev",
    "deploy:function:dev": "npm run deploy:dev -- function -f",
    "deploy:prd": "npm run deploy -- --stage prd",
    "deploy:function:prd": "npm run deploy:prd -- function -f"
  },
  "author": {
    "name": "Icaro Silva",
    "email": "ikaromarlon@gmail.com",
    "url": "https://www.linkedin.com/in/ikaromarlon/"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.3",
    "joi": "^17.11.0",
    "mongodb": "^6.3.0",
    "uuid-by-string": "^4.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.484.0",
    "@aws-sdk/client-ses": "^3.484.0",
    "@aws-sdk/client-sqs": "^3.484.0",
    "@shelf/jest-mongodb": "^4.2.0",
    "@types/jest": "^29.5.11",
    "dotenv": "^16.3.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "serverless": "^3.38.0",
    "serverless-apigateway-service-proxy": "^2.2.1",
    "serverless-cloudside-plugin": "^1.0.6",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^13.3.2",
    "standard": "^17.1.0"
  },
  "standard": {
    "env": [
      "node",
      "jest"
    ]
  }
}
